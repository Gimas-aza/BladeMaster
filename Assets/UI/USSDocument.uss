    transition-duration: 0.3s;
}

.GameMenu__LabelH1 {
    align-self: center;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    width: 200px;
    height: 100px;
    justify-content: center;
    -unity-text-align: middle-center;
    font-size: 65px;
    -unity-font-style: bold;
    text-overflow: clip;
    white-space: nowrap;
    color: rgb(255, 130, 37);
    -unity-text-outline-width: 2px;
    -unity-text-outline-color: rgb(23, 59, 69);
}

.GameMenu__LabelWin {
    width: 100%;
    font-size: 35px;
    color: rgb(248, 237, 237);
    display: none;
}

.GameMenu__LabelLose {
    font-size: 35px;
    white-space: normal;
    width: 100%;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    color: rgb(180, 63, 63);
    -unity-text-outline-color: rgb(23, 59, 69);
    -unity-text-outline-width: 1px;
    display: none;
}

.GameMenu__LabelH2 {
    font-size: 45px;
    width: 100%;
}

.PauseMenu__Button {
    margin-bottom: 30px;
}
